svgmd {

Drawing = Sequence

Sequence (Sequence) = Element (arrow Element)*
Element (Element) = Circle | Box | words
Box (Box) = "[" Sequence* "]"
Circle (Circle) = "(" Sequence* ")"
arrow (arrow) = "->"

words = word+
word (word) = ~separator letter alnum*
separator (separator) = "[" | "]" | "(" | ")" | arrow
}
